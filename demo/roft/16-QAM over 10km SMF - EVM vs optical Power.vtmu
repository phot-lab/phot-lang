        <TEXTBOX __key="texts" font="sansserif" fontsize="2" height="10.4" horizjust="left" instanceId="*TB3" text="For proper simulation results, please ensure that \nTimeWindow x SampleRateQAM = 2^n \nTimeWindow x BitRateQAM / BitsPerSymbol = M" textcol="red" vertjust="center" width="47.4">
