    </STATEDEF>
    <STATEDEF __key="states" category="Global" desc="Allows simulations with the number of the signal samples whose greatest prime factor (GPF) does not exceed the specified limit. For example, for 150 samples the GPF is equal to 5 (150 = 2x3x5x5). If the signal is resampled during simulation, the new sample rate may be automatically adjusted (increased) to satisfy the above condition. Holding the GPF limit is important to ensure high speed of the fast Fourier transform that is widely used in simulations. The highest speed is achieved with the default setting 2, which is equivalent to the condition that the number of samples is an integer power of two. It is recommended to set the GPF limit not larger than 13. Setting it to -1 will deactivate the GPF limiting, which allows to simulate an arbitrary number of samples in the signal." label="GreatestPrimeFactorLimit" objectId="GreatestPrimeFactorLimit" path="" type="int">
      <SETTING __key="default" exprType="standard" state="GreatestPrimeFactorLimit" value="2"/>
      <UNITDEF __key="unit" type="int" unit=""/>
      <RANGEDEF __key="range" action="abort" bounds="open" maximum="inf" minimum="-1"/>
    </STATEDEF>
    <STATEDEF __key="states" category="Global" desc="Represents inband noise by noise bins if ON, otherwise adds noise to sampled signals." label="InBandNoiseBins" objectId="InBandNoiseBins" type="enumeration">
      <ENUMDEF __key="enumerations" value="ON"/>
      <ENUMDEF __key="enumerations" value="OFF"/>
      <SETTING __key="default" exprType="standard" state="InBandNoiseBins" value="OFF"/>
      <UNITDEF __key="unit" type="enumeration" unit=""/>
    </STATEDEF>
    <STATEDEF __key="states" category="Global" desc="Selects between Aperiodic blocks and Periodic blocks." label="BoundaryConditions" objectId="BoundaryConditions" type="enumeration">
      <ENUMDEF __key="enumerations" value="Periodic"/>
      <ENUMDEF __key="enumerations" value="Aperiodic"/>
      <SETTING __key="default" exprType="standard" state="BoundaryConditions" value="Periodic"/>
      <UNITDEF __key="unit" type="enumeration" unit=""/>
    </STATEDEF>
    <STATEDEF __key="states" category="Global" desc="Logical information is generated by transmitters and used to reconstruct the transmitted data for BER estimation and clock recovery. If set to ON, logical information is generated and kept in memory for each simulation run. If set to CurrentRun, only the information for the current run is kept (data from previous runs is removed to reduce memory consumption). If set to OFF, logical information is not generated. LogicalInformation = OFF is particularly useful for Photonic Circuits simulations." label="LogicalInformation" objectId="LogicalInformation" type="enumeration">
      <ENUMDEF __key="enumerations" value="ON"/>
      <ENUMDEF __key="enumerations" value="OFF"/>
      <ENUMDEF __key="enumerations" value="CurrentRun"/>
      <SETTING __key="default" exprType="standard" state="LogicalInformation" value="ON"/>
      <UNITDEF __key="unit" type="enumeration" unit=""/>
    </STATEDEF>
    <STATEDEF __key="states" category="Global" desc="Sets the optical bandwidth of the sampled signals. Equal to their SampleRate." label="SampleModeBandwidth" objectId="SampleModeBandwidth" type="float">
