    <STATEDEF __key="states" category="Global" desc="Sets the duration of a block of samples." label="TimeWindow" objectId="TimeWindow" type="float">
      <SETTING __key="default" exprType="standard" state="TimeWindow" unit="s" value="512/32e6"/>
